# platform :ios, '9.0'

target 'facebookSample' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for facebookSample
  pod 'FacebookCore'
  pod 'FacebookLogin'
  pod 'FacebookShare'
	
  target 'facebookSampleTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'facebookSampleUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
